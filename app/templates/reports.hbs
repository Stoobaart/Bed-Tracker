<div class="page">
  <div class="reports">
    <div class="breadcrumbs">
      <LinkTo @route="dashboard" @tagName="span">
        <img src="/assets/images/hospital.svg" alt="">
        <div class="text">{{t "breadcrumbs.dashboard"}}</div>
      </LinkTo>{{t "breadcrumbs.dos-report"}}
    </div>

    <h1>{{t "reports.dos-report.title"}}</h1>
    {{#let this.model.getReport.report as |report|}}

      <div class="reports_report-row">
        <div class="key">{{t "reports.dos-report.totalBedsOfNonSurgeWards"}}</div> 
        <div class="value">{{report.totalBedsOfNonSurgeWards}}</div> 
      </div>

      <div class="reports_report-row">
        <div class="key">{{t "reports.dos-report.totalBedsOfSurgeWards"}}</div> 
        <div class="value">{{report.totalBedsOfSurgeWards}}</div> 
      </div>

      <div class="reports_report-row">
        <div class="key">{{t "reports.dos-report.totalCritcareNursesOfWards"}}</div> 
        <div class="value">{{report.totalCritcareNursesOfWards}}</div> 
      </div>

      <div class="reports_report-row">
        <div class="key">{{t "reports.dos-report.totalOtherRnsOfWards"}}</div> 
        <div class="value">{{report.totalOtherRnsOfWards}}</div> 
      </div>

