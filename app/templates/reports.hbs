        <div class="value">{{report.totalNurseSupportStaffOfWards}}</div> 
      </div>

      <div class="reports_report-row">
        <div class="key">{{t "reports.dos-report.allWardsCanProvideIcsRatios"}}</div> 
        <div class="value">{{if report.allWardsCanProvideIcsRatios "Yes" "No"}}</div> 
      </div>

      <div class="reports_report-row">
